{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "array",
    "default": [],
    "title": "Root Schema",
    "items": {
        "type": "object",
        "default": {},
        "title": "A Schema",
        "required": ["damage", "id"],
        "properties": {
            "damage": {
                "type": "object",
                "default": {},
                "title": "The damage Schema",
                "required": ["magicAttack", "normalAttack", "specialAttack"],
                "properties": {
                    "magicAttack": {
                        "type": "object",
                        "default": {},
                        "title": "The magicAttack Schema",
                        "required": ["aMag", "bMag", "cDmg"],
                        "properties": {
                            "aMag": {
                                "type": "number",
                                "default": 0.0,
                                "title": "The aMag Schema",
                                "examples": [0.3499999940395355]
                            },
                            "bMag": {
                                "type": "number",
                                "default": 0.0,
                                "title": "The bMag Schema",
                                "examples": [0.6000000238418579]
                            },
                            "cDmg": {
                                "type": "number",
                                "default": 0.0,
                                "title": "The cDmg Schema",
                                "examples": [10.0]
                            }
                        },
                        "examples": [
                            {
                                "aMag": 0.3499999940395355,
                                "bMag": 0.6000000238418579,
                                "cDmg": 10.0
                            }
                        ]
                    },
                    "normalAttack": {
                        "type": "object",
                        "default": {},
                        "title": "The normalAttack Schema",
                        "required": ["aMag", "bMag"],
                        "properties": {
                            "aMag": {
                                "type": "number",
                                "default": 0.0,
                                "title": "The aMag Schema",
                                "examples": [1.0]
                            },
                            "bMag": {
                                "type": "number",
                                "default": 0.0,
                                "title": "The bMag Schema",
                                "examples": [1.0]
                            }
                        },
                        "examples": [
                            {
                                "aMag": 1.0,
                                "bMag": 1.0
                            }
                        ]
                    },
                    "specialAttack": {
                        "type": "object",
                        "default": {},
                        "title": "The specialAttack Schema",
                        "required": ["aMag", "bMag", "cDmg"],
                        "properties": {
                            "aMag": {
                                "type": "number",
                                "default": 0.0,
                                "title": "The aMag Schema",
                                "examples": [2.0]
                            },
                            "bMag": {
                                "type": "number",
                                "default": 0.0,
                                "title": "The bMag Schema",
                                "examples": [0.0]
                            },
                            "cDmg": {
                                "type": "number",
                                "default": 0.0,
                                "title": "The cDmg Schema",
                                "examples": [30.0]
                            }
                        },
                        "examples": [
                            {
                                "aMag": 2.0,
                                "bMag": 0.0,
                                "cDmg": 30.0
                            }
                        ]
                    }
                },
                "examples": [
                    {
                        "magicAttack": {
                            "aMag": 0.3499999940395355,
                            "bMag": 0.6000000238418579,
                            "cDmg": 10.0
                        },
                        "normalAttack": {
                            "aMag": 1.0,
                            "bMag": 1.0
                        },
                        "specialAttack": {
                            "aMag": 2.0,
                            "bMag": 0.0,
                            "cDmg": 30.0
                        }
                    }
                ]
            },
            "id": {
                "type": "string",
                "default": "",
                "title": "The id Schema",
                "examples": [""]
            }
        },
        "examples": [
            {
                "damage": {
                    "magicAttack": {
                        "aMag": 0.3499999940395355,
                        "bMag": 0.6000000238418579,
                        "cDmg": 10.0
                    },
                    "normalAttack": {
                        "aMag": 1.0,
                        "bMag": 1.0
                    },
                    "specialAttack": {
                        "aMag": 2.0,
                        "bMag": 0.0,
                        "cDmg": 30.0
                    }
                },
                "id": ""
            }
        ]
    },
    "examples": [
        [
            {
                "damage": {
                    "magicAttack": {
                        "aMag": 0.3499999940395355,
                        "bMag": 0.6000000238418579,
                        "cDmg": 10.0
                    },
                    "normalAttack": {
                        "aMag": 1.0,
                        "bMag": 1.0
                    },
                    "specialAttack": {
                        "aMag": 2.0,
                        "bMag": 0.0,
                        "cDmg": 30.0
                    }
                },
                "id": ""
            }
        ]
    ]
}
